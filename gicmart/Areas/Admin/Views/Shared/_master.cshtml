    <style type="text/css">
        /* Clearing floats */
        .cf:before,
        .cf:after {
            content: " ";
            display: table;
        }

        .cf:after {
            clear: both;
        }

        .cf {
            *zoom: 1;
        }

        /* Mini reset, no margins, paddings or bullets */
        .menu,
        .submenu {
            margin: 0;
            padding: 0;
            list-style: none;
        }

        /* Main level */
        .menu {
            margin: 24px auto;
            margin-left: auto;
            width: 800px;
            width: -moz-fit-content;
            width: -webkit-fit-content;
            width: fit-content;
            margin-left: 4%;
        }

            .menu > li {
                background: #34495e;
                float: left;
                position: relative;
                transform: skewX(0deg); /*25*/
            }

            .menu a {
                display: block;
                color: #fff;
                text-transform: uppercase;
                text-decoration: none;
                font-family: Arial, Helvetica;
                font-size: 14px;
            }

            .menu li:hover {
                background: #e74c3c;
            }

            .menu > li > a {
                transform: skewX(0deg); /*-25*/
                padding: 1em 2em;
            }

        /* Dropdown */
        .submenu {
            position: absolute;
            width: 220px; /*200*/
            left: 45%; /*50%*/
            margin-left: -100px;
            transform: skewX(0deg); /*-25*/
            transform-origin: left top;
        }

            .submenu li {
                background-color: #34495e;
                position: relative; /*relative*/
                overflow: hidden;
            }

            .submenu > li > a {
                padding: 1em 1em; /*1,2*/
            }

            .submenu > li::after {
                content: '';
                position: absolute; /* absolute  */
                top: -125%;
                height: 100%;
                width: 100%;
                box-shadow: 0 0 50px rgba(0, 0, 0, .9);
            }

            /* Odd stuff */
            .submenu > li:nth-child(odd) {
                transform: skewX(0deg) translateX(0); /*-25*/
            }

                .submenu > li:nth-child(odd) > a {
                    transform: skewX(0deg); /*25*/
                }

                .submenu > li:nth-child(odd)::after {
                    right: 0%; /*-50%*/
                    transform: skewX(0deg) rotate(3deg); /*-25*/
                }

            /* Even stuff */
            .submenu > li:nth-child(even) {
                transform: skewX(0deg) translateX(0); /*25*/
            }

                .submenu > li:nth-child(even) > a {
                    transform: skewX(0deg); /*-25*/
                }

                .submenu > li:nth-child(even)::after {
                    left: 0%; /*-50%*/
                    transform: skewX(0deg) rotate(3deg); /*25*/
                }

            /* Show dropdown */
            .submenu,
            .submenu li {
                opacity: 0;
                visibility: hidden;
            }

                .submenu li {
                    transition: .2s ease transform;
                }

        .menu > li:hover .submenu,
        .menu > li:hover .submenu li {
            opacity: 1;
            visibility: visible;
        }

            .menu > li:hover .submenu li:nth-child(even) {
                transform: skewX(0deg) translateX(0px); /*25*/ /*-15*/
            }

            .menu > li:hover .submenu li:nth-child(odd) {
                transform: skewX(0deg) translateX(0px); /*-25*/ /*-15*/
            }
    </style>
